{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "nestjs boilerplate",
  "author": "cabinkid07@gmail.com",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "cross-env NODE_ENV=dev pm2 start dist/main.js",
    "start:docker:local": "cross-env NODE_ENV=dev nest start dist/main.js",
    "start:dev:local": "nodemon",
    "start:prod": "cross-env NODE_ENV=prod pm2 start dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.379.1",
    "@aws-sdk/s3-request-presigner": "^3.379.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/throttler": "^3.1.0",
    "@nestjs/typeorm": "^9.0.1",
    "@types/bcrypt": "^5.0.0",
    "aligoapi": "^1.1.3",
    "aws-sdk": "^2.1426.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cheerio": "1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "crypto-js": "4.2.0",
    "date-fns": "^3.6.0",
    "express-session": "^1.17.3",
    "google-auth-library": "^9.8.0",
    "google-trends-api": "4.9.2",
    "handlebars": "^4.7.8",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.2",
    "lz-string": "^1.5.0",
    "mysql2": "^2.3.3",
    "nest-winston": "^1.8.0",
    "node-cron": "^3.0.3",
    "pako": "2.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-naver": "^1.0.6",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.11",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@types/crypto-js": "4.2.2",
    "@types/express": "^4.17.13",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.11",
    "@types/pako": "2.0.3",
    "@types/passport-naver": "^1.0.4",
    "@types/zen-observable": "^0.8.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
